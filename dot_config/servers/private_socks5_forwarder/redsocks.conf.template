base {
    log_debug = off;
    log_info = on;
    daemon = on;
    redirector = iptables;
}

redsocks {
    local_ip = 127.0.0.1;
    local_port = ${REDSOCKS_PORT};
    type = socks5;
    ip = ${UPSTREAM_HOST};
    port = ${UPSTREAM_PORT};
    login = "${UPSTREAM_USER}";
    password = "${UPSTREAM_PASS}";
}
